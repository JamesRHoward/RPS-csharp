    <link rel="stylesheet" href="Content/css/styles.css" type="text/css">

  </head>
  <body>
    <form id="player-one-input" action="/result" method="post">
      <p>Select Rock, Paper or <em>Scissor</em></p>
      <div id="player-one-turn">
        <label for="player-one">Player One!</label>
        <input type="password" name="player-one" id="player-one">
        <button type="button" id="p1button">Submit</button>
      </div>
      <div id="player-two-turn">
      <label for="player-two">Player Two!</label>
      <input type="password" name="player-two" id="player-two">
      <button type="submit" id="p2button">Submit</button>
    </div>
  </form>
  </body>
</html>
